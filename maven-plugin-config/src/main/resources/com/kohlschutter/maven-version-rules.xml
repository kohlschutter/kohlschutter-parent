<ruleset comparisonMethod="maven"
  xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 https://www.mojohaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
  <ignoreVersions>
    <ignoreVersion type="regex">.*[\-\.]beta.*</ignoreVersion>
    <ignoreVersion type="regex">.*[-\.]alpha[0-9\.]*</ignoreVersion>
    <ignoreVersion type="regex">.*-b201[0-9]*</ignoreVersion>
    <ignoreVersion type="regex">.*-M[0-9]*</ignoreVersion>
    <ignoreVersion type="regex">.*-rc[0-9]*</ignoreVersion>
    <ignoreVersion type="regex">.*jre[678]</ignoreVersion>
  </ignoreVersions>
  <rules>
    <!--
      <rule groupId="com.mycompany.maven" comparisonMethod="maven">
      <ignoreVersions>
      <ignoreVersion type="regex">.*-RELEASE</ignoreVersion>
      <ignoreVersion>2.1.0</ignoreVersion>
      </ignoreVersions>
      </rule>
    -->
  </rules>
</ruleset>
